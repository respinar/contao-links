{
    "name": "respinar/contao-links",
    "description": "Links module for Contao",
    "keywords": ["contao", "link"],
    "type": "contao-module",
    "homepage": "https://respinar.com",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Hamid Abbaszadeh",
            "email": "info@respinar.com",
            "homepage": "http://respinar.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/respinar/contao-links/issues"
    },
    "require": {
        "php": ">=5.4",
        "contao/core-bundle":"~3.5 || ~4.4",
        "contao-community-alliance/composer-plugin":"^2.4.1 || ~3.0"
    },
    "autoload": {
        "psr-4": {
            "Respinar\\Links\\": "src/library/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "src": "system/modules/links"
            },
            "transifex": {
                "project": "contao-links",
                "prefix": "master-",
                "languages_cto": "languages",
                "languages_tx": ".tx"
            }
        }
    }
}
