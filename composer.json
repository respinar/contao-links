{
    "name": "respinar/contao-links",
    "description": "Links bundle for Contao",
    "keywords": ["contao", "link"],
    "type": "contao-bundle",
    "homepage": "https://respinar.com",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Hamid Peywasti",
            "email": "hamid@respinar.com",
            "homepage": "http://respinar.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/respinar/contao-links/issues"
    },
    "require": {
        "php": "^8.0",
        "contao/core-bundle":"^4.13 || ^5.2"
    },
    "autoload": {
        "psr-4": {
            "Respinar\\LinksBundle\\": "src/"
        },
        "classmap": [
            "contao/"
        ],
        "exclude-from-classmap": [
            "contao/config/",
            "contao/dca/",
            "contao/languages/",
            "contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Respinar\\LinksBundle\\ContaoManager\\Plugin"
    }
}
