{
    "name": "respinar/links",
    "description":"Links module for Contao",
    "keywords":["contao", "link"],
    "type": "contao-module",
    "homepage":"https://github.com/respinar/links",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"Hamid Abbaszadeh",
            "email":"info@respinar.com",
            "homepage":"http://respinar.org",
            "role":"Developer"
        }
    ],
    "support":{
        "issues":"https://github.com/respinar/links/issues"
    },
    "require": {
        "php":">=5.3.2",
        "contao/core":">=3.2,<4-dev",
        "contao-community-alliance/composer-plugin": "~2.0"
    },
    "autoload": {
        "classmap": [""]
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/links"
            },
            "transifex": {
                "project": "contao-links",
                "prefix": "master-",
                "languages_cto": "languages",
                "languages_tx": ".tx"
            }
        }
    }
}
